<h1>Edit Project Form</h1>  

<%= form_for @project do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :deadline %>
    <%= f.text_field :deadline %>

    <%= f.label :location %>
    <%= f.text_field :location %>

    <%= f.submit %>
<% end %>