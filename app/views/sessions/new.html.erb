<div class="form-container">

<%= render "shared/errors"%>

  <%= form_for :session, url: creator_login_path do |f| %>
    <div class="form-input">
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>

    <div class="form-input">
        <%= f.label :password %>
        <%= f.text_field :password %>
    </div>

    <%= f.submit %>
    
  <% end %>
</div>