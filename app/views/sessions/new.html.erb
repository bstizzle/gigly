<% if flash[:errors]%>
    <%= flash[:errors]%>
<% end %>

<div class="form-container">

  <%= form_for :session, url: creator_login_path do |f| %>
    <div class="form-input">
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>

    <div class="form-input">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <div class="form-btn">
      <%= f.submit "Log In" %>
    </div>
  <% end %>
</div>